{
    "name": "@webbeetechnologies/frontend-starter-rn",
    "version": "0.0.1",
    "main": "node_modules/expo/AppEntry.js",
    "publishConfig": {
        "private": "restricted",
        "main": "./lib/commonjs/index.js",
        "module": "./lib/module/index.js",
        "types": "./lib/typescript/index.d.ts"
    },
    "scripts": {
        "lint": "eslint .",
        "type-check": "tsc --noEmit",
        "prepare:husky": "husky install",
        "start": "expo start",
        "android": "expo start --android",
        "ios": "expo start --ios",
        "web": "expo start --web",
        "cleanup": "rm -rf lib prepublish",
        "prepare": "bob build",
        "package": "mkdir -p dist && npm pack --pack-destination=dist",
        "bundle": "yarn cleanup && yarn prepare",
        "build": "yarn bundle && yarn package",
        "do-publish": "npm publish ./dist/webbeetechnologies-bamboo-template-$npm_package_version.tgz --access=private",
        "release": "yarn build && yarn run do-publish"
    },
    "files": [
        "lib",
        "!src"
    ],
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "@babel/preset-react": "^7.18.6",
        "@react-native-community/eslint-config": "^3.2.0",
        "@types/lodash.debounce": "^4.0.7",
        "@types/react": "^18.0.14",
        "@types/react-native": "^0.70.6",
        "eslint": "^8.31.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.27.4",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-simple-import-sort": "^8.0.0",
        "expo": "~47.0.9",
        "husky": "^8.0.3",
        "prettier": "^2.7.1",
        "react": "^18.1.0",
        "react-dom": "^18.1.0",
        "react-native": "^0.70.5",
        "react-native-builder-bob": "^0.17.1",
        "react-native-web": "^0.18.9",
        "typescript": "^4.6.3"
    },
    "peerDependencies": {
        "expo": "^47.0.9",
        "react": "^18.1.0",
        "react-dom": "^18.1.0",
        "react-native": "^0.70.5",
        "react-native-vector-icons": "^9.2.0",
        "react-native-web": "^0.18.9"
    },
    "react-native-builder-bob": {
        "source": "src",
        "output": "lib",
        "targets": [
            [
                "commonjs",
                {
                    "configFile": true
                }
            ],
            [
                "module",
                {
                    "configFile": true
                }
            ],
            [
                "typescript",
                {
                    "project": "tsconfig.build.json"
                }
            ]
        ]
    },
    "directories": {
        "lib": "lib"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/webbeetechnologies/bamboo-core.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/webbeetechnologies/bamboo-core/issues"
    },
    "homepage": "https://github.com/webbeetechnologies/bamboo-core#readme",
    "description": ""
}
